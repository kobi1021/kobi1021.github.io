{
  "name": "kobi1021-github-io-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kobi1021.github.io",
  "dependencies": {
    "bulma": "^0.7.2",
    "bulma-extensions": "^4.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest-dom": "^3.0.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1",
    "react-testing-library": "^5.4.2",
    "three": "^0.99.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build && cp -a build/. .",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.2.0",
    "@storybook/addon-actions": "^5.0.8",
    "@storybook/addon-links": "^5.0.8",
    "@storybook/addons": "^5.0.8",
    "@storybook/react": "^5.0.8",
    "gh-pages": "^2.0.1"
  },
  "peerDependencies": {
    "ajv": "^6.5.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
